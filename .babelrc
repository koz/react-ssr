{
  "presets": [[
    "es2015", {
      "loose": true,
      "modules": false
    }],
    "stage-2",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-es2015-modules-commonjs",
    "transform-object-rest-spread",
    "transform-async-to-generator",
    "transform-runtime"
  ]
}
