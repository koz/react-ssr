{
  "name": "react-ssr",
  "version": "0.0.1",
  "description": "React project with server-side-rendering, HMR, redux and code splitting",
  "main": "index.js",
  "scripts": {
    "clean": "cross-env rm -rf ./build",
    "build": "cross-env npm run clean && npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/client.babel.js",
    "production": "cross-env NODE_ENV=production babel-node ./server/index.js",
    "development": "cross-env NODE_ENV=development babel-node ./server/index.js"
  },
  "keywords": [
    "react",
    "ssr",
    "hmr",
    "redux",
    "code",
    "splitting",
    "universal"
  ],
  "author": "Gabriel Kozlowaski",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "lodash-webpack-plugin": "^0.11.4",
    "path": "^0.12.7",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8"
  },
  "dependencies": {
    "express": "^4.15.3",
    "history": "^4.6.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "shortid": "^2.2.8",
    "tachyons": "^4.7.4",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
